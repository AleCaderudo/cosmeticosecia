{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      },
    {
      "src": "back/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/back/server.js"
    }
  ]
}


